{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "bun": "1.1.21"
    },
    "ios-simulator": {
      "extends": "development",
      "ios": {
        "simulator": false
      },
      "bun": "1.1.21"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "bun": "1.1.21"
    },
    "production": {
      "bun": "1.1.21"
    },
    "build-and-maestro-test": {
      "distribution": "internal",
      "withoutCredentials": true,
      "config": "build-and-maestro-test.yml",
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "bun": "1.1.21"
    }
  },
  "submit": {
    "production": {}
  }
}