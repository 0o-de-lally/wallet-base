{
  "expo": {
    "name": "Carpe",
    "slug": "wallet-base",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "walletbase",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#1a1a1f",
    "androidStatusBar": {
      "backgroundColor": "#1a1a1f",
      "barStyle": "light-content"
    },
    "androidNavigationBar": {
      "backgroundColor": "#1a1a1f",
      "barStyle": "light-content"
    },
    "ios": {
      "supportsTablet": true,
      "buildNumber": "2",
      "bundleIdentifier": "com.olcarpe.walletbase.beta",
      "infoPlist": {
        "NSFaceIDUsageDescription": "This app uses Face ID to secure your wallet credentials.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.carpe.wallet_base",
      "permissions": [
        "USE_FINGERPRINT",
        "USE_BIOMETRIC"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ccd33245-8aca-43cc-89d5-d224e10b1298"
      }
    },
    "owner": "ol-carpe"
  }
}
